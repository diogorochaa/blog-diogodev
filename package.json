    "autoprefixer": "10.4.14",
    "contentlayer": "^0.3.2",
    "eslint-config-next": "13.4.7",
    "next": "13.4.7",
    "next-compose-plugins": "^2.2.1",
    "next-contentlayer": "^0.3.2",
    "next-pwa": "^5.6.0",
    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "reading-time": "^1.5.0",
